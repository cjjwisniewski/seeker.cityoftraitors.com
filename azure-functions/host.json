{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Information", // Ensure function results are logged
      "Function": "Information"     // Ensure function-specific logs are captured
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle.Preview", // Or specific version if needed
    "version": "[4.*, 5.0.0)" // Use appropriate bundle version
  },
  "extensions": {
    "http": {
      // Optional: Configure default route prefix if needed (default is 'api')
      // "routePrefix": ""
    }
  }
}
